{
  "name": "real_api_web_test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-css": "tailwindcss build -i src/style.css -o public/style.css",
    "watch-css": "tailwindcss -i src/style.css -o public/style.css --watch",
    "sync": "browser-sync start --proxy http://localhost:8080 --files 'src/**/*' 'public/**/*'",
    "dev": "concurrently \"npm run watch-css\" \"npm run serve\" \"npm run sync\"",
    "production": "tailwindcss build -i src/style.css -o public/style.css && vue-cli-service build"
  },
  "dependencies": {
    "axios": "1.6.7",
    "babylonjs": "^7.3.1",
    "core-js": "^3.8.3",
    "mitt": "^3.0.1",
    "real_api": "^1.1.31",
    "real_api_bbl": "^1.0.40",
    "three": "^0.168.0",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "browser-sync": "^2.29.3",
    "browser-sync-webpack-plugin": "^2.3.0",
    "concurrently": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
